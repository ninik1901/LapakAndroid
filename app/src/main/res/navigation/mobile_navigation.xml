<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.projek.fragment.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_daftarsimpan"
        android:name="com.example.projek.fragment.SimpanFragment"
        android:label="@string/title_simpan"
        tools:layout="@layout/fragment_simpan" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.projek.fragment.NotifFragment"
        android:label="notif"
        tools:layout="@layout/fragment_notif" />

    <fragment
        android:id="@+id/nav_donasiBuku"
        android:name="com.example.projek.DonasiBuku"
        android:label="@string/title_donasi"
        tools:layout="@layout/activity_donasi_buku" />

    <fragment
        android:id="@+id/nav_jadwal"
        android:name="com.example.projek.JadwalLapak"
        android:label="@string/title_jadwal"
        tools:layout="@layout/activity_jadwal_lapak" />
    <fragment
        android:id="@+id/nav_simpan"
        android:name="com.example.projek.fragment.SimpanFragment"
        android:label="Simpan"
        tools:layout="@layout/fragment_simpan" />

    <fragment
        android:id="@+id/nav_akun"
        android:name="com.example.projek.fragment.AkunFragment"
        android:label="Akun"
        tools:layout="@layout/fragment_akun" />

</navigation>
