<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.projek.fragment.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.projek.fragment.SimpanFragment"
        android:label="@string/title_simpan"
        tools:layout="@layout/fragment_simpan" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.projek.fragment.NotifFragment"
        android:label="@string/title_notifikasi"
        tools:layout="@layout/fragment_notif" />

    <fragment
        android:id="@+id/navigation_akun"
        android:name="com.example.projek.fragment.AkunFragment"
        android:label="@string/title_akun"
        tools:layout="@layout/fragment_akun" />
</navigation>
